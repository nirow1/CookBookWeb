@model dynamic

<h1>Recipe Details</h1>

<table>
    <tr>
        <td>Name:</td>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <td>Source:</td>
        <td>@Model.Source</td>
    </tr>
    <tr>
        <td>Score:</td>
        <td>@Model.Score</td>
    </tr>
    <tr>
        <td>Category:</td>
        <td>@Model.Category</td>
    </tr>
    <tr>
        <td>Last Cooked:</td>
        <td>@Model.LastCooked</td>
    </tr>
    <tr>
        <td>Guides:</td>
        <td>
            <ul>
                @foreach (var guide in Model.GuideList)
                {
                    <li>@guide.TotalProtein</li>
                }
            </ul>
        </td>
    </tr>
</table>

<div>
    <a asp-action="Index">Back to List</a>
</div>
